    "@graphql-tools/merge": "^8.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "exif": "^0.6.0",
    "express": "^4.18.2",
    "express-validator": "^6.14.3",
    "graphql-tag": "^2.12.6",
    "helmet": "^6.0.1",
    "mongoose": "^7.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "randomstring": "^1.2.3",
    "sharp": "^0.31.3"
  },
